{
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": "off",
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 8,
          "endOfLine": "auto"
        }
      ]
    }
  }